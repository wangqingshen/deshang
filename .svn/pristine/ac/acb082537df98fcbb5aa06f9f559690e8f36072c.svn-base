.am-topbar {
	background-color: #fff;
	height: 60px;
	min-width: 1100px;
}
.am-topbar-1 {
	max-width: 1100px;
	margin: 0 auto;
	height: 60px
}
.am-slider{
	width: 100%;
	min-width: 1100px;
}
.am-topbar, .am-topbar a {
	color: #333;
	font-size: 14px;
	font-family: '微软雅黑'
}
.am-topbar-brand {
	height: 60px;
	line-height: 55px;
	padding: 0
}
.am-topbar-brand a {
	display: inline-block;
	width: 161px;
	height: 43px;
	background: url(../images/logo_top.png) no-repeat;
	text-indent: -9999px;
	vertical-align: middle
}
.am-nav-pills > li {
	height: 60px;
	margin-left: 70px !important
}
ul.am-slides li {
	width: 688px;
	margin-right: 0px;
	float: left;
	display: block
}
ul.am-direction-nav {
	width: 1100px;
	display: block;
	position: absolute;
	height: 100%;
	top: 0;
	/*z-index: 0;*/
	margin: auto;
	left: 50%;
	margin-left: -550px
}
#ds-part1 {
	background: url(../images/images5.png) repeat;
	min-width: 1100px;
	width: 100%;
	height: 570px
}
.am-content-1 {
	background: url(../images/images3.png);
	width: 1100px;
	height: 472px;
	position: relative;
	margin: 0 auto;
	top: 42px
}
.am-content-header-logo {
	width: 116px;
	height: 71px;
	background: url(../images/images2.png);
	position: absolute;
	left: 264px;
	top: 38px
}
.am-content-header-t {
	width: 294px;
	height: 54px;
	background: url(../images/images1.png);
	position: absolute;
	left: 50%;
	top: 26px;
	font-size: 40px;
	font-weight: bolder;
	color: #2f318b;
	text-align: center;
	margin-left: -145px
}
.am-content-desc {
	width: 840px;
	height: 230px;
	position: absolute;
	left: 132px;
	top: 112px
}
.am-content-desc em {
	display: inline-block;
	float: left;
	font-size: 16px;
	font-weight: bolder;
	margin-right: 20px;
	font-style: normal
}
.am-content-desc span {
	display: inline-block;
	float: left;
	font-size: 12px;
	line-height: 30px;
	width: 690px
}
#ds-part2 {
	width: 100%;
	height: 420px
}
.am-sec-content {
	background: url(../images/images8.jpg);
	width: 1100px;
	height: 420px;
	margin: 0 auto
}
#ds-part3 {
	width: 100%;
	height: 420px;
	background: url(../images/images10.jpg) repeat;
	margin-bottom: 120px;
	background-size: contain;
}
.am-thr-content {
	width: 1100px;
	height: 420px;
	background: #a9c5ca;
	margin: 0 auto;
	position: relative
}
i.arrow-up{
	border-color: rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #a9c5ca;
    border-style: dashed dashed solid;
    border-width: 15px;
    display: block;
    font-size: 0;
    height: 0;
    left: 50%;
    line-height: 0;
    position: absolute;
    top: -30px;
    width: 0;
}
.am-thr-content-title {
	width: 100%;
	position: relative;
	top: 40px
}
.am-thr-content-cht {
	font-size: 40px;
	color: #fff;
	font-weight: bolder;
	display: block;
	width: 100%;
	text-align: center;
	font-style: normal;
	line-height: 40px
}
.am-thr-content-ent {
	font-size: 30px;
	color: #e62129;
	font-weight: bolder;
	display: block;
	width: 100%;
	text-align: center;
	font-style: normal;
	line-height: 30px
}
.am-thr-content-logo {
	width: 38px;
	height: 76px;
	background: url(../images/images4.png);
	position: absolute;
	top: 56px;
	left: 50%;
	margin-left: 90px
}
.am-thr-content-desc {
	font-size: 12px;
	color: #fff;
	line-height: 30px;
	width: 656px;
	height: 200px;
	margin: 65px auto 0
}
.ds-footer {
	width: 100%;
	height: 140px;
	background: #464646;
	min-width: 1100px
}
.am-for-content {
	width: 1100px;
	height: 100%;
	margin: auto
}
.am-for-l {
	float: left;
	margin-top: 40px
}
.am-for-logo {
	width: 160px;
	height: 38px;
	background: url(../images/logo_bottom.png) no-repeat;
	text-indent: -9999px;
	vertical-align: middle;
	margin-bottom: 0
}
.am-for-l small, .am-for-r small {
	font-size: 12px;
	color: #999
}
.am-for-r {
	float: right;
	margin-top: 50px
}
.am-for-r p {
	font-size: 12px;
	color: #999;
	margin-bottom: 0;
	position: relative
}
.am-for-r p>i {
	background: url(../images/icon/icon-email.png);
	width: 17px;
	height: 14px;
	display: inline-block;
	top: 2px;
	position: absolute;
	left: -30px
}
.am-prev {
	background: url(../images/icon/l.png);
	height: 50px !important;
	width: 50px !important;
}
.am-next {
	background: url(../images/icon/r.png);
	height: 50px !important;
	width: 50px !important;
}